{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "description": "An Electron-based AV Scheduler desktop application built with Next.js",
  "author": "theNoobCoder0x01 <patelhrishikesh2000@gmail.com>",
  "main": "dist/main.js",
  "build": {
    "appId": "com.avscheduler.desktop",
    "productName": "BAPS AV Scheduler",
    "defaultArch": "universal",
    "compression": "maximum",
    "files": [
      "dist/api-server/**",
      "dist/lib/**",
      "dist/models/**",
      "dist/main.js",
      "dist/preload.js",
      "out/**",
      "public/**",
      "package.json"
    ],
    "directories": {
      "buildResources": "build"
    },
    "mac": {
      "defaultArch": "universal",
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "api:dev": "npx ts-node-dev --respawn --transpile-only --project tsconfig.server.json --rs api-server/standalone.ts",
    "api:build": "tsc --project tsconfig.server.json",
    "dev:local": "concurrently \"npm run api:dev\" \"npm run dev\" --names \"API,WEB\" --prefix-colors \"blue,green\" --kill-others-on-fail",
    "dev:api-only": "npm run api:dev",
    "dev:web-only": "npm run dev",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:build": "npm run build:clean && npm run build:static && npm run build:electron && npm run build:package",
    "electron:build-win": "npm run build:clean && npm run build:static && npm run build:electron && electron-builder --win",
    "electron:build-linux": "npm run build:clean && npm run build:static && npm run build:electron && electron-builder --linux",
    "build:clean": "rm -rf dist out .next",
    "build:static": "next build",
    "build:electron": "tsc && npm run api:build",
    "build:package": "electron-builder --mac --arm64 --x64",
    "test:routing": "node scripts/test-routing.js"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.0",
    "@next/swc-wasm-nodejs": "13.5.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@reduxjs/toolkit": "^2.0.1",
    "@tailwindcss/container-queries": "^0.1.1",
    "@types/node": "20.6.2",
    "@types/react": "^18.2.25",
    "@types/react-dom": "18.2.7",
    "@types/ws": "^8.5.10",
    "autoprefixer": "10.4.15",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.3.0",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.1",
    "express": "^4.18.2",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.446.0",
    "next": "13.5.1",
    "next-themes": "^0.3.0",
    "postcss": "8.4.30",
    "react": "18.2.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.0",
    "react-redux": "^9.0.4",
    "react-resizable-panels": "^2.1.3",
    "recharts": "^2.12.7",
    "sonner": "^1.5.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vaul": "^0.9.9",
    "winston": "^3.17.0",
    "ws": "^8.16.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "cross-env": "^7.0.3",
    "electron": "^36.2.1",
    "electron-builder": "^26.0.12"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9",
    "utf-8-validate": "^6.0.5"
  }
}